<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".clothingActivity">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="352dp"
        android:layout_height="73dp"
        android:fontFamily="@font/bungee"
        android:text="CLICK HERE TO SEE YOUR ACTIVE CLOTHING TENDER"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/userclothpast"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <Button
        android:id="@+id/userclothactive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btngradient"
        android:text="Active"
        app:layout_constraintBottom_toTopOf="@+id/userclothpast"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34"
        app:layout_constraintVertical_bias="0.196" />

    <Button
        android:id="@+id/userclothpast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="108dp"
        android:background="@drawable/btngradient"
        android:text="Past"
        app:layout_constraintBottom_toTopOf="@+id/clothingBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/clothingBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btngradient"
        android:padding="7dp"
        android:text="+New"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="352dp"
        android:layout_height="73dp"
        android:fontFamily="@font/bungee"
        android:text="CLICK HERE TO SEE YOUR CLOSED CLOTHING TENDER"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/userclothpast"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userclothactive"
        app:layout_constraintVertical_bias="0.569" />

</androidx.constraintlayout.widget.ConstraintLayout>